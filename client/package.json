{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cd dist && serve .",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "npm run build && now --public && now alias"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "serve": "^6.5.6",
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.10",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.10",
    "@vue/cli-service": "^3.0.0-beta.10",
    "vue-template-compiler": "^2.5.13"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "now": {
    "name": "saintplay-miniflix",
    "alias": "saintplay-miniflix"
  }
}
